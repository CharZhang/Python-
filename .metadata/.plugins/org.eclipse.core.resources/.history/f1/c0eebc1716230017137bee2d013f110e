#coding:utf-8
import urllib2
import urllib


class HtmlDowanloader(object):
    
    def download(self,url):
        if url is None:
            return None
        params = urllib.urlencode([('input_text', '重庆'), ('input_text', '南京')])
        response=urllib2.urlopen(url，params)
        if response.getcode()!=200:
            
            return None
        return response.read()
        